<Window x:Class="TwinkleStar.TestWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TwinkleStar.TestWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <TabControl>
        <TabItem Header="验证码">
            <StackPanel HorizontalAlignment="Left">
                <Image Name="img" DockPanel.Dock="Left" Width="90" Height="30"/>
                <Button Name="btnClick" DockPanel.Dock="Right" Width="65" Height="23" Content="刷新"/>
            </StackPanel>
        </TabItem>
    </TabControl>
</Window>
